{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": ".", // Add this line
    "paths": { // Add this section
      "@raydium-io/raydium-sdk-v2": ["node_modules/@raydium-io/raydium-sdk-v2"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}